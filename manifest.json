{
    "manifest_version": 3,
    "name": "Simple Notes",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "dist/content.js"
            ],
            "type": "module"
        }
    ],
    "icons": {
        "32": "icons/icons8-notes-32.png",
        "16": "icons/icons8-notes-16.png",
        "48": "icons/icons8-notes-48.png",
        "128": "icons/icons8-notes-128.png"
    },
    "action": {
        "default_popup": "dist/index.html"
    },
    "background": {
        "service_worker": "dist/background.js"
    }
}